   "source": [
    "n = int(input())\n",
    "idx=1\n",
    "while(n-idx>0):\n",
    "    n-=idx\n",
    "    idx+=1\n",
    "if idx%2==0:\n",
    "    print(f'{n}/{idx+1-n}')\n",
    "else:\n",
    "    print(f'{idx+1-n}/{n}')"
   ]
