   "source": [
    "n,m = map(int,input().split())\n",
    "castle=[]\n",
    "\n",
    "for _ in range(n):\n",
    "    castle.append(input())\n",
    "\n",
    "row = []\n",
    "col = []\n",
    "\n",
    "for i in range(n):\n",
    "    row.append(\"X\" not in castle[i])\n",
    "\n",
    "for j in range(m):\n",
    "    col.append(\"X\" not in [castle[i][j] for i in range(n)])    \n",
    "\n",
    "print(max(sum(row),sum(col)))"
   ]
  }
